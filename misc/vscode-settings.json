{
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "atomKeymap.promptV3Features": true,
  "breadcrumbs.enabled": false,
  "editor.accessibilitySupport": "off",
  "editor.colorDecorators": false,
  "editor.cursorBlinking": "solid",
  "editor.cursorWidth": 6,
  "editor.folding": false,
  "editor.fontFamily": "Operator Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 11.7,
  "editor.formatOnSave": true,
  "editor.glyphMargin": false,
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.matchBrackets": false,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.quickSuggestions": true,
  "editor.quickSuggestionsDelay": 30,
  "editor.renderControlCharacters": false,
  "editor.renderWhitespace": "none",
  "editor.rulers": [100, 80],
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 35,
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 500,
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/bower_components": true,
    "**/CVS": true
  },
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/bower_components/**": true,
    "**/dist/**": true,
    "**/node_modules/**": true,
    "**/tmp/**": true
  },
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "gitlens.mode.statusBar.enabled": false,
  "gitlens.views.fileHistory.location": "scm",
  "gitlens.views.repositories.enabled": false,
  "javascript.autoClosingTags": false,
  "javascript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.validate.enable": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "path-intellisense.autoSlashAfterDirectory": true,
  "prettier.printWidth": 100,
  "prettier.singleQuote": true,
  "subtleBrackets.style": {
    "borderWidth": "2px"
  },
  "sync.forceUpload": true,
  "sync.gist": "your key",
  "telemetry.enableTelemetry": false,
  "terminal.integrated.fontSize": 10,
  "terminal.integrated.scrollback": 15000,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.preferences.quoteStyle": "single",
  "typescript.tsserver.log": "verbose",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.newWindowDimensions": "maximized",
  "window.restoreWindows": "all",
  "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}",
  "window.zoomLevel": 0.2414244710871944,
  "workbench.activityBar.visible": false,
  "workbench.colorCustomizations": {
    "scrollbarSlider.background": "#5b6578",
    "editor.selectionBackground": "#135564",
    "editor.selectionHighlightBackground": "#135564"
  },
  "workbench.colorTheme": "One Dark Pro",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.openPositioning": "last",
  "workbench.editor.tabCloseButton": "off",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "none",
  "workbench.tree.renderIndentGuides": "always",
  "terminal.integrated.rendererType": "dom",
  "gitlens.menus": {
    "editor": {
      "blame": false,
      "clipboard": true,
      "compare": true,
      "details": false,
      "history": false,
      "remote": false
    },
    "editorGroup": {
      "blame": false,
      "compare": false
    },
    "editorTab": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "explorer": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "scmGroup": {
      "compare": true,
      "openClose": true,
      "stash": true,
      "stashInline": true
    },
    "scmItem": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true,
      "stash": true
    }
  },
  "html.autoClosingTags": false,
  "html.format.wrapLineLength": 100,
  "update.showReleaseNotes": false,
  "gitlens.currentLine.enabled": false,
  "typescript.autoClosingTags": false
}

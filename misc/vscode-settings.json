  "docker.showStartPage": false,
  "editor.accessibilitySupport": "off",
  "editor.colorDecorators": true,
  "editor.cursorBlinking": "solid",
  "editor.cursorWidth": 10,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true

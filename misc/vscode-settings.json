{
  "[dotenv]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[properties]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "ActiveFileInStatusBar.fullpath": false,
  "application.shellEnvironmentResolutionTimeout": 100,
  "breadcrumbs.enabled": false,
  "breadcrumbs.symbolPath": "off",
  "colorize.languages": [
    "typescript",
    "javascript",
    "css",
    "scss",
    "javascriptreact",
    "typescriptreact",
    "json"
  ],
  "debug.javascript.codelens.npmScripts": "never",
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.colorDecorators": true,
  "editor.cursorBlinking": "solid",
  "editor.cursorWidth": 10,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.folding": false,
  "editor.fontFamily": "Operator Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 13.7,
  "editor.formatOnSave": false,
  "editor.formatOnPaste": false,
  "editor.glyphMargin": false,
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.linkedEditing": true,
  "editor.matchBrackets": "always",
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on",
    "other": "on"
  },
  "editor.suggest.preview": true,
  "editor.quickSuggestionsDelay": 60,
  "editor.renderControlCharacters": false,
  "editor.renderWhitespace": "none",
  "editor.rulers": [80],
  "editor.suggest.insertMode": "insert",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "emmet.excludeLanguages": ["typescriptreact", "javascript", "typescript"],
  "emmet.showExpandedAbbreviation": "never",
  "errorLens.excludePatterns": ["**.md"],
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 35,
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/bower_components": true,
    "**/CVS": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/bower_components/**": true,
    "**/dist/**": true,
    "**/build/**": true,
    "**/node_modules/**": true,
    "**/tmp/**": true
  },
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "gitlens.menus": {
    "editor": {
      "blame": false,
      "clipboard": true,
      "compare": true,
      "details": false,
      "history": false,
      "remote": false
    },
    "editorGroup": {
      "blame": false,
      "compare": false
    },
    "editorTab": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "explorer": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "scmGroup": {
      "compare": true,
      "openClose": true,
      "stash": true,
      "stashInline": true
    },
    "scmItem": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true,
      "stash": true
    }
  },
  "gitlens.mode.statusBar.enabled": false,
  "html.autoClosingTags": true,
  "html.format.wrapLineLength": 100,
  "javascript.autoClosingTags": false,
  "javascript.preferences.quoteStyle": "single",
  "javascript.preferences.jsxAttributeCompletionStyle": "auto",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.validate.enable": true,
  "json.schemas": [
    {
      "name": "vscode-expo-manifest",
      "url": "file:///Users/tim/Library/Application%20Support/Code/User/globalStorage/bycedric.vscode-expo/manifest-45.0.0.json",
      "fileMatch": ["app.json", "app.config.json"]
    }
  ],
  "liveServer.settings.donotShowInfoMsg": true,
  "prettier.printWidth": 100,
  "prettier.singleQuote": true,
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/tmp": true,
    "**.recording.json": true,
    "**/build": true,
    "**/dist": true
  },
  "search.useGlobalIgnoreFiles": true,
  "search.useIgnoreFiles": true,
  "search.useParentIgnoreFiles": true,
  "security.workspace.trust.enabled": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.scrollback": 15000,
  "typescript.autoClosingTags": false,
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.preferences.jsxAttributeCompletionStyle": "auto",
  "typescript.preferences.quoteStyle": "single",
  "typescript.tsserver.log": "off",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.validate.enable": true,
  "update.showReleaseNotes": false,
  "vscodeGoogleTranslate.preferredLanguage": "English",
  "window.newWindowDimensions": "maximized",
  "window.restoreWindows": "all",
  "window.title": "${activeEditorShort}${separator}${rootName}",
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#135564",
    "editor.selectionHighlightBackground": "#135564",
    "scrollbarSlider.background": "#5b6578"
  },
  "workbench.colorTheme": "One Dark Pro Darker",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.openPositioning": "last",
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.decorations.badges": false,
  "workbench.editor.decorations.colors": true,
  "workbench.editor.wrapTabs": true,
  "workbench.editor.pinnedTabSizing": "compact",
  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "none",
  "workbench.tree.renderIndentGuides": "always",
  "workbench.tree.indent": 10,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.preferredDarkColorTheme": "One Dark Pro",
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "rules": {
      "*.mutable": {
        "underline": false
      }
    }
  },
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "diffEditor.ignoreTrimWhitespace": true,
  "window.zoomLevel": 0.5227586988632231,
  "workbench.editor.tabActionCloseVisibility": false,
  "errorLens.enabled": false,
  "github.copilot.editor.enableAutoCompletions": true,
  "git.openRepositoryInParentFolders": "always",
  "workbench.activityBar.location": "hidden",
  "coverage-gutters.showRulerCoverage": true,
  "coverage-gutters.showLineCoverage": true,
  "testing.gutterEnabled": false,
  "window.confirmSaveUntitledWorkspace": false
}

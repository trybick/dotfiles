    {
      "name": "vscode-expo-manifest",
      "url": "file:///Users/tim/Library/Application%20Support/Code/User/globalStorage/bycedric.vscode-expo/manifest-44.0.0.json",
      "fileMatch": ["app.json", "app.config.json"]
    }
